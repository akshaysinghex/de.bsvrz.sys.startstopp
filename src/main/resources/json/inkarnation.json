{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"applikation": {
			"type": "string"
		},
		"aufrufParameter": {
			"items": {
				"type": "string"
			},
			"type": "array"
		},
		"inkarnationsTyp": {
			"type": "string",
			"enum": [
				"dav",
				"wrapped",
				"extern"
			],
			"default": "dav"
		},
		"inkarnationsName": {
			"type": "string"
		},
        "initialize": {
            "type": "boolean",
            "default": false
        },
		"startArt": {
			"$ref": "startArt.json"
		},
		"startBedingung": {
			"$ref": "startBedingung.json"
		},
		"startFehlerVerhalten": {
			"$ref": "startFehlerVerhalten.json"
		},
		"stoppBedingung": {
			"$ref": "stoppBedingung.json"
		},
		"stoppFehlerVerhalten": {
			"$ref": "stoppFehlerVerhalten.json"
		}
	},
	"required": [
		"startBedingungen",
		"aufrufParameter",
		"applikation",
		"startArt",
		"stoppBedingungen",
		"startFehlerVerhalten",
		"stoppFehlerVerhalten",
		"inkarnationsName"
	]}
	
}
